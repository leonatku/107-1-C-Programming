#include<stdio.h>
int main(){
int h1,h2;
float answer1,answer2;
while(h1>= 0,h2>=0){
printf("Enter height: ");
scanf("%d %d",&h1,&h2);
if(h1>=0,h2>=0){
for(int i=h1;i<=h2;i++){
    answer1=(i*i/10000.)*18.5;
    answer2=(i*i/10000.)*24;
printf("%d cm: %.1f ~ %.1f (kg)\n",i,answer1,answer2);
    }}else
    printf("Bye! coding by 406531169");}
    return 0;
}
